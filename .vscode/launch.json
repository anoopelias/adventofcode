{
    "configurations": [
    {
        "name": "Launch go file",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${file}"
    },
    {
        "name": "ts-node",
        "type": "node",
        "request": "launch",
        "args": [
            "${relativeFile}"
        ],
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "cwd": "${workspaceRoot}",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart"
    }
    ]
}